    "start:compatibility": "es-dev-server --app-index index.html --compatibility all --node-resolve --open --watch"
  },
  "dependencies": {
    "@material/mwc-icon-button": "^0.9.1",
    "direwolf-elements": "0.0.0",
    "direwolf-ifml-elements": "~0.0.0",
    "direwolf-istar-elements": "~0.0.0",
    "direwolf-modeler": "0.0.0",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^0.15.1",
    "es-dev-server": "^1.32.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.27.14"
  }
}
