  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/direwolf/direwolf-app.git"
  },
  "scripts": {
    "start:build": "cd dist && es-dev-server --open --base-path '/spaces'",
    "build": "rimraf dist && rollup -c rollup.config.js",
